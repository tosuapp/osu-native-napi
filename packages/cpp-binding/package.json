    "dist/Release/*.node",
    "dist/Release/*.dll"
  ],
  "scripts": {
    "build": "cmake -B build && cmake --build build --config Release",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.1",
    "typescript": "^5.9.3"
  }
}
