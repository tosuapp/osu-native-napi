{
  "name": "@tosuapp/osu-native-napi",
  "version": "1.0.0",
  "author": "Cherry",
  "main": "index.js",
  "types": "osu-native.d.ts",
  "license": "GPL-3.0",
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "exports": {
    ".": {
      "types": "./osu-native.d.ts",
      "require": "./index.js",
      "import": "./index.mjs"
    }
  },
  "files": [
    "index.js",
    "index.mjs",
